{
  "name": "Derek Johstons - Mintbean Hackathon Extension",
  "description": "Script Runner Chrome Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "favicon-32x32.png"
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "pageloader.js",
      "background.js"
    ],
    "persistent": true
  }
}